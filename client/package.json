{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "lint:file": "eslint --max-warnings=0",
    "preview": "vite preview",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "format:file": "prettier --check",
    "codegen": "graphql-codegen",
    "codegen:watch": "graphql-codegen --watch",
    "test-component": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:codegen": "playwright codegen http://localhost:7000",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install"
  },
  "dependencies": {
    "@apollo/client": "^3.13.5",
    "@daypilot/daypilot-lite-react": "^3.32.1",
    "@fontsource/inter": "^5.2.5",
    "@fontsource/nunito": "^5.2.5",
    "@parcel/watcher": "^2.5.1",
    "@tailwindcss/vite": "^4.0.16",
    "axios": "^1.9.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "framer-motion": "^12.6.1",
    "graphql": "^16.10.0",
    "lucide-react": "^0.484.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.4",
    "react-router-dom": "^7.4.0",
    "react-toastify": "^11.0.5",
    "tailwindcss": "^4.0.16"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.0",
    "@graphql-codegen/typescript-react-apollo": "^4.3.2",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vitest": "^3.1.4"
  }
}